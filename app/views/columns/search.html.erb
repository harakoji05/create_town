<div class="container main">
	<div class="row">
		<div class="col col-sm-2">
			<%= render 'column-search' %>
		</div>
		<div class="col col-sm-10">
			<h4><span class="under">検索結果</span></h4>
			<% if @columns.present? %>
				<%= render 'index', columns: @columns %>
			<% else %>
					<p>検索結果がありません。</p>
			<% end %>
			</div>
			<div  class="row">
				<%= paginate @columns %>
			</div>
		</div>
	</div>
</div>
